QGroupBox {
    color: #303030;
    background-color: #323232;
    border:1px solid transparent;
    border-radius:8px;
    margin-top: 6px;
    padding-left:  6px;
    padding-right: 6px;
    padding-top:   12px;
    padding-bottom: 24px;
}

QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position:top left;
	color: #ffffff;
	border-top 1em transparent;
	border-bottom: 0.1em inset #76797c;
}

QPushButton {
    color: #eff0f1;
    border: 1px solid #76797c;
    border-radius: 2px;
    padding: 2px;
    background-color: #323232;
}

QPushButton:hover {
    color: #eff0f1;
    border: 1px solid #7e7e7e;
    background-color: #323232;
}

QPushButton:disabled {
    color: #454545;
    border: 1px solid #76797c;
    background-color: #323232;
}

QPushButton:pressed {
    color: #eff0f1;
    border: 1px inset #8C8C8C;
    margin-top: 2px;
    background-color: #454a4f;
}

QPushButton:checked {
    color: #eff0f1;
    border: 1px inset #76797c;
    margin-top: 0px;
    background-color: #626568;
}

QSlider::groove:horizontal {
    border: 1px inset #323232;
    height: 3px;
    background: 2c2c2c;
    margin: 2px 0;
    border-radius: 1px;
}

QSlider::handle:horizontal {
    background: 2c2c2c;
    border: 1px solid #323232;
    width: 7px;
    margin: -6px 0; /* handle is placed by default on the contents rect of the groove. Expand outside the groove */
    border-radius: 2px;
}

QCheckBox#checkItem {
}

QCheckBox#checkItem::indicator {
    margin: 0px;
    padding: 4px;
    width: 16px;
    height: 16px;
}
